{
    "name": "@suryapratap/angular-datetime-picker",
    "version": "11.0.3",
    "description": "Angular Date Time Picker",
    "keywords": [
        "Angular",
        "datepicker",
        "date picker",
        "timepicker",
        "time picker",
        "datetime picker",
        "date time picker",
        "material",
        "ngx"
    ],
    "author": "Maintained and updated by Surya Pratap froked from Daniel Moncada, original implementatiom by Daniel Pan",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build picker",
        "test": "ng test picker",
        "test-with-coverage": "ng test --watch=false --no-progress --browsers=ChromeHeadlessNoSandbox --code-coverage=true picker",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build_lib": "ng build picker --prod=true && copyfiles README.md dist/picker",
        "build_css": "sass  --style=compressed projects/picker/src/sass/picker.scss  dist/picker/assets/style/picker.min.css",
        "npm_pack": "cd dist/picker && npm pack",
        "package": "npm run build_lib && npm run build_css && npm run npm_pack"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/suryapratap/date-time-picker.git"
    },
    "dependencies": {
        "@angular/animations": "^11.0.1",
        "@angular/common": "^11.0.1",
        "@angular/compiler": "^11.0.1",
        "@angular/core": "^11.0.1",
        "@angular/forms": "^11.0.1",
        "@angular/platform-browser": "^11.0.1",
        "@angular/platform-browser-dynamic": "^11.0.1",
        "@angular/router": "^11.0.1",
        "dayjs": "^1.9.6",
        "rxjs": "^6.6.3",
        "zone.js": "~0.11.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1100.2",
        "@angular-devkit/schematics": "^11.0.1",
        "@angular/cdk": "^11.0.0",
        "@angular/cli": "^11.0.2",
        "@angular/compiler-cli": "^11.0.1",
        "@angular/language-service": "^11.0.1",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "^2.0.8",
        "@types/moment-timezone": "^0.5.12",
        "@types/node": "^12.12.29",
        "codecov": "^3.6.5",
        "codelyzer": "^6.0.0",
        "copyfiles": "^2.4.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.1.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^11.0.2",
        "protractor": "~7.0.0",
        "sass": "^1.29.0",
        "ts-node": "^8.6.2",
        "tslint": "~6.1.0",
        "typescript": "^4.0.5"
    },
    "peerDependencies": {}
}
